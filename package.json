{
  "name": "@tvlabs/wdio-service",
  "version": "0.1.2",
  "description": "WebdriverIO service that provides a better integration into TV Labs",
  "author": "Regan Karlewicz <regan@tvlabs.ai>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tv-labs/wdio-tvlabs-service.git"
  },
  "keywords": [
    "wdio-plugin",
    "wdio-service",
    "tvlabs",
    "tv-labs",
    "appium"
  ],
  "bugs": {
    "url": "https://github.com/tv-labs/wdio-tvlabs-service/issues"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --module commonjs --outDir cjs; echo '{\"type\":\"commonjs\"}' > cjs/package.json",
    "build:esm": "tsc --module NodeNext --moduleResolution node16 --outDir esm; echo '{\"type\":\"module\"}' > esm/package.json",
    "start": "ts-node src/index.ts",
    "dev": "ts-node --transpile-only src/index.ts",
    "clean": "rm -rf cjs && rm -rf esm",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint",
    "test": "vitest --config vitest.config.ts --coverage"
  },
  "type": "module",
  "types": "./esm/index.d.ts",
  "exports": {
    "require": "./cjs/src/index.js",
    "import": "./esm/src/index.js"
  },
  "main": "cjs/src/index.js",
  "typeScriptVersion": "5.8.2",
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.10",
    "@types/phoenix": "^1.6.6",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitest/coverage-v8": "^3.0.9",
    "@wdio/globals": "^9.12.1",
    "@wdio/logger": "^9.4.4",
    "@wdio/types": "^9.10.1",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.0.9",
    "webdriverio": "^9.12.1"
  },
  "dependencies": {
    "chalk": "^5.1.2",
    "phoenix": "^1.7.20",
    "ws": "^8.18.1"
  }
}
